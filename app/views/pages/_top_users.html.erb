<% if @top_users.empty? %>
		<a href="#" class="collection-item">No top user yet!</a>
<% else %>
	<% @top_users.each do |full_name, created_at, links_count| %>
		<li class="collection-item avatar">
			<%= image_tag("user.png", class:"circle", alt:"Profile Picture") %>
			<span class="title"><%= full_name %></span>
			 <p class="small">Member Since: <%= time_ago_in_words(created_at) %><br>
				 Shortened Links: <%= links_count %> <br>
			  </p>
			</li>
	<% end %> 
<% end %>