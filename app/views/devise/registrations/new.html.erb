
<div class="container">
	<div class="row center">
		<div class="col s12">
		<h2 class="tab">Sign up</h2>
	</div>
</div>

<div class="row center jumbo">
	
	<div class="nav-wrapper">
			
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %> 
    <div class="card">
      		
        <div class="row">
        	<div class="col s11">
        			<%= f.text_field :full_name, autofocus: true, placeholder: 'Full Name', style:'margin:12px;margin-left: 25px;' %>
        		</div>
        		
        		<div class="col s11">
        			<%= f.email_field :email, autofocus: true, placeholder: 'Email', style:'margin:12px;margin-left: 25px;' %>
        		</div>   		
        </div>
        		
        <div class="row">
        		<div class="col s11">
    <%= f.password_field :password, autocomplete: "off", placeholder: 'Password: min of 6 characters', style:'margin:12px;margin-left: 25px;' %>
        		</div>
        		
        		<div class="col s11">

    <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: 'Re-type Password', style:'margin:12px;margin-left: 25px;' %>
        		</div>   		
        		
        	</div>
        </div>

        	
        </div>
        
        
    <div class="row">
          <div class="col s12">
          	
			<div class="actions">
				<%= f.submit "Register", class: "btn waves-effect waves-light", id:'reg' %>
				
			</div>
  			
  </div>
        </div>    

<% end %>

<%= render "devise/shared/links" %>
    </div>

</div>
	
</div>








